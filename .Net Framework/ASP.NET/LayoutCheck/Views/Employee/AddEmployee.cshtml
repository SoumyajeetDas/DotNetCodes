
@{
    Layout = "~/Views/Shared/_Layout1.cshtml";
    var p = Page;

    var a = p[0];
    var b = p[1];
    var c = p[2];
}

<!DOCTYPE html>

<div>
    <p>This is Add Employee Page</p>
    @foreach(var k in p)
            {
                <h3>@k</h3>
            }

    @for(int i=0;i<3;i++)
            {
                <h2>@p[i]</h2>
            }


    @a<br />
    @b<br />
    @c<br />
   

    
</div>
